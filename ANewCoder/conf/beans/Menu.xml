<?xml version="1.0" encoding="UTF-8"?>
<entitys xmlns="http://www.wamdy.com/entitys"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.wamdy.com/entitys entitys.xsd">
	<entity>
	    <package>cn.cmschina.ibp.client.client</package>
	    <name>Menu</name>
	    <note>用于生成框架左边的菜单</note>
	    <tableName>menu</tableName>
	    <fields>
	        <keyField>
	            <field>
	                <name>menu_id</name>
	                <type>String</type>
	                <dbType>menuId</dbType>
	                <note>菜单表的主键</note>
	            </field>
	        </keyField>
	        <normalField>
	            <field>
	                <name>name</name>
	                <type>String</type>
	                <note>菜单名</note>
	                <dbType>name</dbType>
				</field>
				<field>
					<name>treecode</name>
					<type>String</type>
	                <note>树形编码</note>
	                <dbType>treeCode</dbType>
				</field>
				<field>
				    <name>ismenuitem</name>
				    <type>Boolean</type>
				    <note>标识是否为菜单项</note>
				    <dbType>isMenuItem</dbType>
				</field>
				<field>
					<name>icon</name>
					<type>String</type>
				    <note>菜单的图标，菜单</note>
				    <dbType>icon</dbType>
				</field>
				<field>
					<name>url</name>
					<type>String</type>
				    <note>点击菜单后访问的地址</note>
				    <dbType>url</dbType>
				</field>
	        </normalField>
	    </fields>
	</entity>
</entitys>